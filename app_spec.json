{
    "Module" : {
        "Name": "Telegraf",
        "Description": "Telegraf module manifest",
        "SchemaVersion": "0.2",
        "ManifestFile": "${idx:telegraf:$EIS_VERSION}",
        "RunAsUser": "$EIS_UID",
        "RunAsGroup": "$EIS_UID",
        "Resources": {
            "CPU": 100,
            "MemoryMB": 100
        },
        "ExecutionEnv": {
            "AppName": "Telegraf",
            "ETCD_ENDPOINT": "${datastore.endpoint}",
            "CONFIGMGR_CACERT": "${databucket.cacert}",
            "CONFIGMGR_CERT": "${databucket.cert}",
            "CONFIGMGR_KEY": "${databucket.key}",
            "ETCD_PREFIX": "$ETCD_PREFIX",
            "DEV_MODE": "$DEV_MODE",
            "PROFILING_MODE": "$PROFILING_MODE",
            "InfluxDbAppName": "InfluxDBConnector",
            "MQTT_BROKER_HOST": "127.0.0.1",
            "INFLUX_SERVER": "${ep.inputTelegraf.remoteaddress}",
            "INFLUXDB_PORT": "$INFLUXDB_PORT"
        },
        "Endpoints": [
            {
                "Name": "inputTelegraf",
                "Endtype": "client",
                "Protocol": "TCP",
                "DataType": "messages",
                "Link": "influxout"
            }
        ]
    }
}